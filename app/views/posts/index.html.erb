<p id="notice"><%= notice %></p>

<h1>Timeline</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= link_to post.title, post_path(post) %></td>
        <td><%= post.content %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

